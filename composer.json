{
    "name": "rudashi/laravel-history",
    "description": "Eloquent model history for Laravel",
    "keywords": [
        "model",
        "laravel",
        "eloquent",
        "history",
        "log",
        "activity"
    ],
    "authors": [
        {
            "name": "Borys Å»muda",
            "email": "rudashi@gmail.com",
            "homepage": "https://rudashi.github.io",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/rudashi/laravel-history",
    "require": {
        "php": "^7.4 || ^8.1",
        "illuminate/config": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/database": "^8.53 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Rudashi\\LaravelHistory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rudashi\\LaravelHistory\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rudashi\\LaravelHistory\\HistoryServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
