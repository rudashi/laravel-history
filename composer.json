{
    "name": "rudashi/laravel-history",
    "license": "MIT",
    "description": "Eloquent model history for Laravel",
    "keywords": ["model", "laravel", "eloquent", "history"],
    "homepage": "https://github.com/rudashi/laravel-history",
    "authors": [
        {
            "name": "Borys Å»muda",
            "email": "rudashi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "laravel/framework": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3|^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rudashi\\LaravelHistory\\": "src/"
        },
        "classmap": [
            "src/database/migrations"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rudashi\\LaravelHistory\\": "src/",
            "Rudashi\\LaravelHistory\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/migrations"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rudashi\\LaravelHistory\\HistoryServiceProvider"
            ]
        }
    }
}
